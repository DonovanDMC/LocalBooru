<%= form_search(path: post_replacements_path) do |f| %>
  <%= f.input(:md5, label: "MD5") %>
  <%= f.input(:post_id, label: "Post ID") %>
  <%= f.user(:creator) %>
  <%= f.user(:approver) %>
  <%= f.user(:rejector) %>
  <%= f.input("uploader_ip_addr_on_approve", label: "Uploader IP Address On Approve") %>
  <%= f.input(:status, collection: %w[pending rejected approved promoted], include_blank: true) %>
<% end %>

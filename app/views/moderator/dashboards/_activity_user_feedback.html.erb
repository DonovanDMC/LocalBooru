<%= table_for(@dashboard.user_feedbacks) do |t| %>
  <% t.caption { "User Feedback" } %>
  <% t.column("User") do |record| %>
    <%= link_to_user(record.user) %>
  <% end %>
  <% t.column("Message") do |record| %>
    <%= format_text(record.body) %>
  <% end %>
  <% t.column("Date") do |record| %>
    <%= time_ago_in_words_tagged(record.created_at) %>
  <% end %>
<% end %>

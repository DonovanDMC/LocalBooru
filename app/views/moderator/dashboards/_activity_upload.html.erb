<%= table_for(@dashboard.posts) do |t| %>
  <% t.caption { "Uploads" } %>
  <% t.column("User") do |activity| %>
    <%= link_to_user(activity.user) %>
  <% end %>
  <% t.column("Count") do |activity| %>
    <%= link_to(activity.count, post_versions_path(search: { updater_id: activity.user.id })) %>
  <% end %>
<% end %>

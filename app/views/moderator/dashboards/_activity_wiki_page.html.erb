<%= table_for(@dashboard.wiki_pages) do |t| %>
  <% t.caption { "Wiki Page Updates" } %>
  <% t.column("User") do |activity| %>
    <%= link_to_user(activity.user) %>
  <% end %>
  <% t.column("Count") do |activity| %>
    <%= link_to(activity.count, wiki_page_versions_path(search: { updater_id: activity.user.id })) %>
  <% end %>
<% end %>
